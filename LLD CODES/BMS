    }

}

public class BookMyShow{
    ArrayList<Threater> threaters;
    static Hashmap<String,ArrayList<Show>> movieMap;

    private void genMovMap(){
        for(Threater threater:threaters ){
            ArrayList<Show> showArray=threater.shows();
            if(show!=null){
                if(movieMap.contains(show.getMovie().getName())){
                    movieMap.get(show.getMovie().getName()).add(show);
                }
                else{ ArrayList<Show> showArrayList=new ArrayList<> ();
                showArrayList.add(show);
                movieMap.put(show.getMovie().getName,showArrayList);

                }
            }
        }
    }
    private static ArrayList<show> searchShow(){
        if(movieMap.containskey(moviename) ) return above;
        else return null;
    }
}
